#Print week number if you provide week as name

Weekname= "dfthgj"

if Weekname =="Sunday":
    print("Welcome to Sunday week 7")
elif Weekname =="Monday":
    print("Welcome to Monday week 1")
elif Weekname =="Tuesday":
    print("Welcome to Tuesday week 2")
elif Weekname == "Wednesday":
    print("Welcome to Thursday week 3")
elif Weekname =="Thursday":
    print("Welcome to Wednesday week 4")
elif Weekname =="Friday":
    print("Welcome to Friday week 5")
else:
    print("Welcome to Unknown week 6")

